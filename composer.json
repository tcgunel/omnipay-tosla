{
    "name": "tcgunel/omnipay-tosla",
    "type": "library",
    "description": "Omnipay extension for Tosla",
    "keywords": [
        "tosla",
        "omnipay",
        "payment",
        "gateway",
        "3d secure",
        "ödeme",
        "bin sorgulama",
        "ortak ödeme sayfası",
        "iptal",
        "iade"
    ],
    "homepage": "https://github.com/tcgunel/omnipay-tosla",
    "license": "MIT",
    "authors": [
        {
            "name": "Tolga Can Günel",
            "email": "tlgcngnl@gmail.com"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/tcgunel/omnipay-tosla/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\Tosla\\": "src/"
        }
    },
    "require": {
        "php": "^8.0",
        "league/omnipay": "^3",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "omnipay/tests": "^4",
        "brianium/paratest": "*",
        "fzaninotto/faker": "*",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
